{
  "name": "blocks",
  "version": "1.0.0",
  "description": "Real estate API that provides a platform for users to view houses and make enquiries",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_ENV=production && node index.js",
    "client": "npm run dev --prefix client",
